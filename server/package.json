    "@types/cors": "^2.8.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "typescript": "^2.7.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/express": "^4.11.1",
